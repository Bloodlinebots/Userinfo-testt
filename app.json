{
  "name": "Telegram User Info Bot",
  "description": "Telegram bot to show info about users, groups, channels, and bots.",
  "keywords": ["telegram", "pyrogram", "user info", "bot"],
  "stack": "container",
  "env": {
    "BOT_TOKEN": {
      "description": "Your bot token from BotFather",
      "required": true
    },
    "API_ID": {
      "description": "Get this from https://my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Get this from https://my.telegram.org",
      "required": true
    }
  }
}
